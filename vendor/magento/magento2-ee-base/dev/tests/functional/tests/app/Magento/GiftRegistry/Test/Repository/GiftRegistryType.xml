<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\GiftRegistry\Test\Repository\GiftRegistryType">
        <dataset name="default">
            <field name="code" xsi:type="string">code_%isolation%</field>
            <field name="label" xsi:type="string">gift_registry_label%isolation%</field>
            <field name="is_listed" xsi:type="string">Yes</field>
        </dataset>

        <dataset name="text">
            <field name="code" xsi:type="string">code_%isolation%</field>
            <field name="label" xsi:type="string">gift_registry_label%isolation%</field>
            <field name="is_listed" xsi:type="string">Yes</field>
            <field name="attributes" xsi:type="array">
                <item name="dataset" xsi:type="string">text</item>
            </field>
        </dataset>

        <dataset name="select">
            <field name="code" xsi:type="string">code_%isolation%</field>
            <field name="label" xsi:type="string">gift_registry_select%isolation%</field>
            <field name="is_listed" xsi:type="string">Yes</field>
            <field name="attributes" xsi:type="array">
                <item name="dataset" xsi:type="string">select</item>
            </field>
        </dataset>

        <dataset name="date">
            <field name="code" xsi:type="string">code_%isolation%</field>
            <field name="label" xsi:type="string">gift_registry_date%isolation%</field>
            <field name="is_listed" xsi:type="string">Yes</field>
            <field name="attributes" xsi:type="array">
                <item name="dataset" xsi:type="string">date</item>
            </field>
        </dataset>

        <dataset name="country">
            <field name="code" xsi:type="string">code_%isolation%</field>
            <field name="label" xsi:type="string">gift_registry_label%isolation%</field>
            <field name="is_listed" xsi:type="string">Yes</field>
            <field name="attributes" xsi:type="array">
                <item name="dataset" xsi:type="string">country</item>
            </field>
        </dataset>

        <dataset name="event_date">
            <field name="code" xsi:type="string">code_%isolation%</field>
            <field name="label" xsi:type="string">gift_registry_event_date%isolation%</field>
            <field name="is_listed" xsi:type="string">Yes</field>
            <field name="attributes" xsi:type="array">
                <item name="dataset" xsi:type="string">country</item>
            </field>
        </dataset>

        <dataset name="event_country">
            <field name="code" xsi:type="string">code_%isolation%</field>
            <field name="label" xsi:type="string">gift_registry_event_country%isolation%</field>
            <field name="is_listed" xsi:type="string">Yes</field>
            <field name="attributes" xsi:type="array">
                <item name="dataset" xsi:type="string">country</item>
            </field>
        </dataset>

        <dataset name="event_location">
            <field name="code" xsi:type="string">code_%isolation%</field>
            <field name="label" xsi:type="string">gift_registry_event_location%isolation%</field>
            <field name="is_listed" xsi:type="string">Yes</field>
            <field name="attributes" xsi:type="array">
                <item name="dataset" xsi:type="string">country</item>
            </field>
        </dataset>

        <dataset name="role">
            <field name="code" xsi:type="string">code_%isolation%</field>
            <field name="label" xsi:type="string">gift_registry_rolel%isolation%</field>
            <field name="is_listed" xsi:type="string">Yes</field>
            <field name="attributes" xsi:type="array">
                <item name="dataset" xsi:type="string">role</item>
            </field>
        </dataset>
    </repository>
</config>
