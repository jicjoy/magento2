<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\GiftCard\Test\Repository\GiftCardProduct\CheckoutData">
        <dataset name="gift_card_default">
            <field name="options" xsi:type="array">
                <item name="giftcard_options" xsi:type="array">
                    <item name="giftcard_amount" xsi:type="string">option_key_1</item>
                    <item name="giftcard_sender_name" xsi:type="string">Sender_name_%isolation%</item>
                    <item name="giftcard_sender_email" xsi:type="string">Sender_name_%isolation%@example.com</item>
                    <item name="giftcard_recipient_name" xsi:type="string">Recipient_name_%isolation%</item>
                    <item name="giftcard_recipient_email" xsi:type="string">Recipient_name_%isolation%@example.com</item>
                    <item name="giftcard_message" xsi:type="string">Message text %isolation%</item>
                </item>
            </field>
            <field name="qty" xsi:type="string">1</field>
            <field name="cartItem" xsi:type="array">
                <item name="price" xsi:type="string">150</item>
                <item name="qty" xsi:type="string">1</item>
                <item name="subtotal" xsi:type="string">150</item>
            </field>
        </dataset>

        <dataset name="gift_card_50">
            <field name="options" xsi:type="array">
                <item name="giftcard_options" xsi:type="array">
                    <item name="giftcard_amount" xsi:type="string">option_key_1</item>
                    <item name="giftcard_sender_name" xsi:type="string">Sender_name_%isolation%</item>
                    <item name="giftcard_sender_email" xsi:type="string">Sender_name_%isolation%@example.com</item>
                    <item name="giftcard_recipient_name" xsi:type="string">Recipient_name_%isolation%</item>
                    <item name="giftcard_recipient_email" xsi:type="string">Recipient_name_%isolation%@example.com</item>
                    <item name="giftcard_message" xsi:type="string">Message text %isolation%</item>
                </item>
            </field>
            <field name="qty" xsi:type="string">1</field>
            <field name="cartItem" xsi:type="array">
                <item name="price" xsi:type="string">50</item>
                <item name="qty" xsi:type="string">1</item>
                <item name="subtotal" xsi:type="string">50</item>
            </field>
        </dataset>

        <dataset name="gift_card_open_amount">
            <field name="options" xsi:type="array">
                <item name="giftcard_options" xsi:type="array">
                    <item name="custom_giftcard_amount" xsi:type="string">150</item>
                    <item name="giftcard_sender_name" xsi:type="string">Sender_name_%isolation%</item>
                    <item name="giftcard_sender_email" xsi:type="string">Sender_name_%isolation%@example.com</item>
                    <item name="giftcard_recipient_name" xsi:type="string">Recipient_name_%isolation%</item>
                    <item name="giftcard_recipient_email" xsi:type="string">Recipient_name_%isolation%@example.com</item>
                    <item name="giftcard_message" xsi:type="string">Message text %isolation%</item>
                </item>
            </field>
            <field name="qty" xsi:type="string">1</field>
            <field name="cartItem" xsi:type="array">
                <item name="price" xsi:type="string">150</item>
                <item name="qty" xsi:type="string">1</item>
                <item name="subtotal" xsi:type="string">150</item>
            </field>
        </dataset>
    </repository>
</config>
