        <website>
            <selector>[name$="[website_id]"]</selector>
            <strategy>css selector</strategy>
            <input>select</input>
        </website>
        <customer_group>
            <selector>[name$="[customer_group_id]"]</selector>
            <strategy>css selector</strategy>
            <input>select</input>
        </customer_group>
        <browsing_category>
            <selector>[name$='[grant_catalog_category_view]']</selector>
