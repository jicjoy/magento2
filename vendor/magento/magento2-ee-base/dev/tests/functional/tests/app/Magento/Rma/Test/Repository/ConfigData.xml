<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\Config\Test\Repository\ConfigData">
        <dataset name="rma_enable_on_frontend">
            <field name="sales/magento_rma/enabled" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Yes</item>
                <item name="value" xsi:type="number">1</item>
            </field>
            <field name="sales/magento_rma/enabled_on_product" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Yes</item>
                <item name="value" xsi:type="number">1</item>
            </field>
            <field name="sales/magento_rma/use_store_address" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Yes</item>
                <item name="value" xsi:type="number">1</item>
            </field>
        </dataset>

        <dataset name="rma_enable_on_frontend_US_NY">
            <field name="sales/magento_rma/enabled" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Yes</item>
                <item name="value" xsi:type="number">1</item>
            </field>
            <field name="sales/magento_rma/enabled_on_product" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Yes</item>
                <item name="value" xsi:type="number">1</item>
            </field>
            <field name="sales/magento_rma/use_store_address" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">No</item>
                <item name="value" xsi:type="number">0</item>
            </field>
            <field name="sales/magento_rma/store_name" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="value" xsi:type="string">Return Store</item>
            </field>
            <field name="sales/magento_rma/address" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="value" xsi:type="string">Main Street 1</item>
            </field>
            <field name="sales/magento_rma/city" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="value" xsi:type="string">New York</item>
            </field>
            <field name="sales/magento_rma/region_id" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">New York</item>
                <item name="value" xsi:type="number">43</item>
            </field>
            <field name="sales/magento_rma/zip" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="value" xsi:type="number">10010</item>
            </field>
            <field name="sales/magento_rma/country_id" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">United States</item>
                <item name="value" xsi:type="string">US</item>
            </field>
        </dataset>

        <dataset name="rma_enable_on_frontend_US_NY_rollback">
            <field name="sales/magento_rma/enabled" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">No</item>
                <item name="value" xsi:type="number">0</item>
            </field>
            <field name="sales/magento_rma/enabled_on_product" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Yes</item>
                <item name="value" xsi:type="number">1</item>
            </field>
            <field name="sales/magento_rma/use_store_address" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Yes</item>
                <item name="value" xsi:type="number">1</item>
            </field>
        </dataset>
    </repository>
</config>
