<mapping strict="0">
    <wrapper>catalogevent</wrapper>
    <fields>
        <date_start>
            <selector>#event_edit_date_start</selector>
            <input>datepicker</input>
        </date_start>
        <date_end>
            <selector>#event_edit_date_end</selector>
            <input>datepicker</input>
        </date_end>
        <sort_order />
        <display_state composite="1">
            <category_page>
                <selector>#event_edit_display_state_array_1</selector>
                <strategy>css selector</strategy>
                <input>checkbox</input>
            </category_page>
            <product_page>
                <selector>#event_edit_display_state_array_2</selector>
                <strategy>css selector</strategy>
                <input>checkbox</input>
            </product_page>
        </display_state>
    </fields>
</mapping>
