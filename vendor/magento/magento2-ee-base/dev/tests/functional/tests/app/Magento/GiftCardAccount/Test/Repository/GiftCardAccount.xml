<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\GiftCardAccount\Test\Repository\GiftCardAccount">
        <dataset name="default">
            <field name="balance" xsi:type="string">0</field>
            <field name="website_id" xsi:type="array">
                <item name="dataset" xsi:type="string">main_website</item>
            </field>
        </dataset>

        <dataset name="active_redeemable_account">
            <field name="status" xsi:type="string">Yes</field>
            <field name="is_redeemable" xsi:type="string">Yes</field>
            <field name="website_id" xsi:type="array">
                <item name="dataset" xsi:type="string">main_website</item>
            </field>
            <field name="balance" xsi:type="string">10</field>
        </dataset>

        <dataset name="gift_card_account_amount_1">
            <field name="status" xsi:type="string">Yes</field>
            <field name="is_redeemable" xsi:type="string">Yes</field>
            <field name="website_id" xsi:type="array">
                <item name="dataset" xsi:type="string">main_website</item>
            </field>
            <field name="balance" xsi:type="string">1</field>
        </dataset>

        <dataset name="gift_card_account_amount_100">
            <field name="status" xsi:type="string">Yes</field>
            <field name="is_redeemable" xsi:type="string">Yes</field>
            <field name="website_id" xsi:type="array">
                <item name="dataset" xsi:type="string">main_website</item>
            </field>
            <field name="balance" xsi:type="string">100</field>
            <field name="date_expires" xsi:type="string">01/01/2054</field>
            <field name="recipient_email" xsi:type="string">johndoe@example.com</field>
            <field name="recipient_name" xsi:type="string">John Doe</field>
            <field name="recipient_store" xsi:type="string">Default Store View</field>
        </dataset>
    </repository>
</config>
