<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\CatalogEvent\Test\Repository\CatalogEventEntity">
        <dataset name="default">
            <field name="date_start" xsi:type="array">
                <item name="pattern" xsi:type="string">m/d/Y 12:00 a-3 days</item>
            </field>
            <field name="date_end" xsi:type="array">
                <item name="pattern" xsi:type="string">m/d/Y 12:00 a+3 days</item>
            </field>
            <field name="sort_order" xsi:type="string">1</field>
            <field name="display_state" xsi:type="array">
                <item name="category_page" xsi:type="string">Yes</item>
                <item name="product_page" xsi:type="string">Yes</item>
            </field>
            <field name="category_id" xsi:type="array">
                <item name="dataset" xsi:type="string">default_subcategory</item>
            </field>
        </dataset>

        <dataset name="catalog_event_entity_with_category_contains_product">
            <field name="date_start" xsi:type="array">
                <item name="pattern" xsi:type="string">m/d/Y 12:00 a-3 days</item>
            </field>
            <field name="date_end" xsi:type="array">
                <item name="pattern" xsi:type="string">m/d/Y 12:00 a+3 days</item>
            </field>
            <field name="sort_order" xsi:type="string">1</field>
            <field name="display_state" xsi:type="array">
                <item name="category_page" xsi:type="string">Yes</item>
                <item name="product_page" xsi:type="string">Yes</item>
            </field>
            <field name="category_id" xsi:type="array">
                <item name="dataset" xsi:type="string">default_subcategory_with_assigned_simple_product</item>
            </field>
        </dataset>
    </repository>
</config>
