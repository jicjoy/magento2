<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\Config\Test\Repository\ConfigData">
        <dataset name="salesarchive_pending_closed">
            <field name="sales/magento_salesarchive/active" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Yes</item>
                <item name="value" xsi:type="number">1</item>
            </field>
            <field name="sales/magento_salesarchive/order_statuses" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="value" xsi:type="array">
                    <item name="Pending" xsi:type="string">pending</item>
                    <item name="Closed" xsi:type="string">closed</item>
                </item>
            </field>
        </dataset>

        <dataset name="salesarchive_complete">
            <field name="sales/magento_salesarchive/active" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Yes</item>
                <item name="value" xsi:type="number">1</item>
            </field>
            <field name="sales/magento_salesarchive/order_statuses" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string"/>
                <item name="value" xsi:type="string">complete</item>
            </field>
        </dataset>

        <dataset name="salesarchive_all_statuses">
            <field name="sales/magento_salesarchive/active" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Yes</item>
                <item name="value" xsi:type="number">1</item>
            </field>
            <field name="sales/magento_salesarchive/order_statuses" xsi:type="array">
                <item name="scope" xsi:type="string">sales</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="value" xsi:type="array">
                    <item name="Pending" xsi:type="string">pending</item>
                    <item name="Processing" xsi:type="string">processing</item>
                    <item name="Complete" xsi:type="string">complete</item>
                    <item name="Closed" xsi:type="string">closed</item>
                    <item name="Canceled" xsi:type="string">canceled</item>
                    <item name="On Hold" xsi:type="string">holded</item>
                </item>
            </field>
        </dataset>
    </repository>
</config>
