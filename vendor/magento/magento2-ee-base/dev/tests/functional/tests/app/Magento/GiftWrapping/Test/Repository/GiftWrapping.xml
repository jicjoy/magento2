<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\GiftWrapping\Test\Repository\GiftWrapping">
        <dataset name="default">
            <field name="design" xsi:type="string">Gift Wrapping %isolation%</field>
            <field name="website_ids" xsi:type="array">
                <item name="dataset" xsi:type="string">main_website</item>
            </field>
            <field name="status" xsi:type="string">Enabled</field>
            <field name="base_price" xsi:type="string">10</field>
        </dataset>

        <dataset name="enabled">
            <field name="design" xsi:type="string">GiftWrapping%isolation%</field>
            <field name="website_ids" xsi:type="array">
                <item name="dataset" xsi:type="string">main_website</item>
            </field>
            <field name="status" xsi:type="string">Enabled</field>
            <field name="base_price" xsi:type="string">10</field>
        </dataset>

        <dataset name="disabled">
            <field name="design" xsi:type="string">Gift Wrapping%isolation%</field>
            <field name="website_ids" xsi:type="array">
                <item name="dataset" xsi:type="string">main_website</item>
            </field>
            <field name="status" xsi:type="string">Disabled</field>
            <field name="base_price" xsi:type="string">10</field>
        </dataset>
    </repository>
</config>
